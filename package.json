{
  "name": "is-check",
  "version": "1.0.0",
  "description": "micro check library",
  "main": "dist/is.umd.js",
  "exports": {
    ".": {
      "import": "./dist/is.esm.js",
      "require": "./dist/is.cjs.js",
      "default": "./dist/is.umd.js"
    },
    "./type": {
      "import": "./dist/type.esm.js",
      "require": "./dist/type.cjs.js",
      "default": "./dist/type.umd.js"
    },
    "./number": {
      "import": "./dist/number.esm.js",
      "require": "./dist/number.cjs.js",
      "default": "./dist/number.umd.js"
    },
    "./date": {
      "import": "./dist/date.esm.js",
      "require": "./dist/date.cjs.js",
      "default": "./dist/date.umd.js"
    },
    "./env": {
      "import": "./dist/env.esm.js",
      "require": "./dist/env.cjs.js",
      "default": "./dist/env.umd.js"
    },
    "./regexp": {
      "import": "./dist/regexp.esm.js",
      "require": "./dist/regexp.cjs.js",
      "default": "./dist/regexp.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npx husky install",
    "bootstrap": "npm install",
    "reinstall": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && npm run bootstrap",
    "lint": "eslint --ext .js,.mjs,.cjs ./ --max-warnings 0 --cache --cache-location node_modules/.cache/eslint/ --fix",
    "format": "prettier --write .",
    "build": "rimraf dist && rollup -c rollup.config.mjs",
    "commit": "git add -A && git cz && git push",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "is",
    "validate",
    "check",
    "lib",
    "tools",
    "utils"
  ],
  "author": "huatten",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/eslint-parser": "^7.24.8",
    "@babel/preset-env": "^7.24.8",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-terser": "^0.4.4",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vitest/ui": "^0.34.7",
    "commitizen": "^4.3.0",
    "core-js": "^3.37.1",
    "cz-git": "^1.9.4",
    "eslint": "^8.57.0",
    "eslint-config-alloy": "^5.1.2",
    "glob": "^10.4.5",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.19.0",
    "rollup-plugin-filesize": "^10.0.0",
    "standard-version": "^9.5.0",
    "vitepress": "^1.3.1",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
